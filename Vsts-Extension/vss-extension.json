{
    "manifestVersion": 1,
    "id": "Azure-AD-Application-Management",
    "version": "1.0.22",
    "name": "Azure AD Application Management",
    "description": "A VSTS extension with build and release tasks to manage Azure AD Applications.",
    "publisher": "RalphJansen",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services.Cloud"
        }
    ],
    "icons": {
        "default": "images/AdApplication-128.png",
        "large": "images/AdApplication-512.png"
    },
    "categories": [
        "Build and release"
    ],
    "tags": [
        "Azure",
        "Cloud",
        "Active Directory",
        "App Registration",
        "Application"
    ],
    "contributions": [
        {
            "id": "New-Azure-AD-Application",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "New-AzureAdApplication"
            }
        },
        {
            "id": "Get-Azure-AD-Application",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Get-AzureAdApplication"
            }
        },
        {
            "id": "Set-Azure-AD-Application",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Set-AzureAdApplication"
            }
        },
        {
            "id": "Remove-Azure-AD-Application",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Remove-AzureAdApplication"
            }
        }
    ],
    "files": [
        {
            "path": "node_modules/vss-web-extension-sdk/lib",
            "addressable": true,
            "packagePath": "lib"
        },
        {
            "path": "New-AzureAdApplication"
        },
        {
            "path": "Get-AzureAdApplication"
        },
        {
            "path": "Set-AzureAdApplication"
        },
        {
            "path": "Remove-AzureAdApplication"
        }
    ]
}